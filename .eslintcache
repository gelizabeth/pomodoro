newState = 'rest';\n          dispatch({type: 'DEC_ROUNDS'})\n        }\n        else\n          if (state === 'rest') {\n            if (rounds !== 0) {\n              newState = 'work'\n            }\n          }\n        audio.play(); \n        dispatch({ type: 'SET_STATE', state: newState });\n        \n        \n        console.log('did next');\n      }\n    useEffect(() => {\n        \n        const countDown = () => {\n            if (secondsRef.current > 0) {\n                setMinutes(minutesRef.current);\n                setSeconds(secondsRef.current-1);\n            }\n            if (secondsRef.current === 0) {\n                if (minutesRef.current === 0) {\n                    clearInterval(interval);\n                    if(state!=='initial'){ next();};   \n                } else {\n                    setMinutes(minutesRef.current-1);\n                    setSeconds(59);\n                }\n            }\n        }\n\n        const updateTimer = () => {\n            if(rounds!==0){\n                if(state==='work'){\n                    setMinutes(25);\n                    setSeconds(0);\n                }\n                if(state==='rest'){\n                    setMinutes(5);\n                    setSeconds(0);\n                }\n            }   else {\n                setMinutes(0);\n                setSeconds(0);\n            }\n        }\n\n        updateTimer();  \n         var interval = setInterval(countDown, 100);\n         return () => {clearInterval(interval)}\n\n    }, [state]);\n\n    useEffect(()=>{\n        minutesRef.current = minutes;\n        secondsRef.current = seconds;\n    }, [minutes, seconds]);\n\n    const onStartClick = () => { dispatch({ type: 'SET_STATE', state: 'work' })}\n    const onRepeatClick = () => {dispatch({ type: 'RESTART'})};\n\n    return (\n        <>\n        <audio\n           \n          id=\"beep\"\n          preload=\"auto\"\n          \n          src=\"https://raw.githubusercontent.com/freeCodeCamp/cdn/master/build/testable-projects-fcc/audio/BeepSound.wav\"\n        />\n        {rounds === 0 ?\n            <div className=\"container repeat\">\n            <h3 className=\"header\">Great job! Want to repeat?</h3>\n              <button onClick={onRepeatClick}>repeat</button>\n            </div> :\n            <div className=\"container\">\n              <h3 className=\"header\">{header}</h3>\n              <div className=\"tomato-container\">\n        <div className=\"tomato-top\">\n            <div className=\"tomato-leaf\">\n            </div>\n            <div className=\"tomato-leaf\">\n            </div>\n            <div className=\"tomato-leaf\">\n            </div>  \n            \n        </div>\n        <div className=\"tomato\">\n           <div className=\"number-container\">{minutes<10? `0${minutes}`: minutes}</div>\n           <span className=\"tomato-colon\">:</span>\n           <div className=\"number-container\">{seconds<10? `0${seconds}`: seconds}</div>\n           \n       </div>\n       <p className=\"tomato-rounds\">{rounds} Rounds left</p>\n       \n        </div>\n              {state === 'initial' && <button onClick={onStartClick}>Start</button>}\n              {state !== 'initial' && <button onClick={next}>Next</button>}\n    \n            </div>}\n        \n        </>\n        \n    )\n}\n\nexport default Pomodoro\n",{"ruleId":"34","replacedBy":"35"},{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","severity":1,"message":"39","line":1,"column":8,"nodeType":"40","messageId":"41","endLine":1,"endColumn":12},{"ruleId":"38","severity":1,"message":"42","line":11,"column":19,"nodeType":"40","messageId":"41","endLine":11,"endColumn":27},{"ruleId":"43","severity":1,"message":"44","line":77,"column":8,"nodeType":"45","endLine":77,"endColumn":15,"suggestions":"46"},"no-native-reassign",["47"],"no-negated-in-lhs",["48"],"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'dispatch' is assigned a value but never used.",